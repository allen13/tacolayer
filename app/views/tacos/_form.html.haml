= form_for @taco do |f|
  - if @taco.errors.any?
    #error_explanation
      %h2= "#{pluralize(@taco.errors.count, "error")} prohibited this taco from being saved:"
      %ul
        - @taco.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :url
    = f.text_field :url
  .actions
    = f.submit 'Save'
